/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[15])
 */

CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
    Or8Way(in[0..7] = in[0..7], out = o1);
    Or8Way(in[0..7] = in[8..15], out = o2);
    Or(a = o1, b = o2, out = out);
}